<!--pages/components/header.wxml-->
<view class="page">
  <view style='height: 23px; background-color: rgb(231, 244, 255);font-size:12px;padding-left:20px'>
    <navigator style='display:inline-block' open-type="reLaunch" url="../index/index">
      <text style='color:#2d79ce'>首页</text>
    </navigator>
    <text style='padding-left:10px;padding-right:10px;'>|</text>
    <navigator style='display:inline-block' url="../setTel/setTel">
